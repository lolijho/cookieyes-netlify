{
  "framework": "nextjs",
  "buildCommand": "npm run build",
  "devCommand": "npm run dev",
  "installCommand": "npm install",
  "outputDirectory": ".next",
  "cleanUrls": true,
  "functions": {
    "app/api/**/*.ts": {
      "maxDuration": 30
    }
  },

  "ignoreCommand": "git diff --quiet HEAD^ HEAD ./"
}

